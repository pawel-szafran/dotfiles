[
  {
    "key": "alt+cmd+g",
    "command": "editor.action.goToDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "shift+escape",
    "command": "workbench.action.closePanel"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.output.action.clearOutput"
  },
  {
    "key": "shift+cmd+n",
    "command": "explorer.newFile"
  },
  {
    "key": "shift+alt+e",
    "command": "-markdown-table-formatter.enableForCurrentScope"
  },
  {
    "key": "alt+cmd+left",
    "command": "-markdown-table-formatter.moveColumnLeft"
  },
  {
    "key": "alt+cmd+right",
    "command": "-markdown-table-formatter.moveColumnRight"
  },
  {
    "key": "cmd+k cmd+i",
    "command": "-editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+i",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+i",
    "command": "editor.debug.action.showDebugHover",
    "when": "editorTextFocus && inDebugMode"
  },
  {
    "key": "cmd+k cmd+i",
    "command": "-editor.debug.action.showDebugHover",
    "when": "editorTextFocus && inDebugMode"
  },
  {
    "key": "shift+cmd+f",
    "command": "search.action.openNewEditor"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences"
  },
  {
    "key": "alt+cmd+r",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && editorLangId != http"
  }
]